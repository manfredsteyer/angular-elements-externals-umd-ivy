{
  "name": "webcomp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "postinstall-original": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node copy-wc-polyfill.js && node copy-bundles.js",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle --keep-polyfills --keep-styles",
    "build:webcomp:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project webcomp --single-bundle --keep-polyfills --keep-styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.15",
    "@angular/common": "~9.0.0-next.15",
    "@angular/compiler": "~9.0.0-next.15",
    "@angular/core": "~9.0.0-next.15",
    "@angular/elements": "^9.0.0-next.15",
    "@angular/forms": "~9.0.0-next.15",
    "@angular/platform-browser": "~9.0.0-next.15",
    "@angular/platform-browser-dynamic": "~9.0.0-next.15",
    "@angular/router": "~9.0.0-next.15",
    "@webcomponents/custom-elements": "^1.3.0",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "copy": "^0.3.2",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^8.1.5",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~9.0.0-next.19",
    "@angular/compiler-cli": "~9.0.0-next.15",
    "@angular/language-service": "~9.0.0-next.15",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.6.4"
  }
}
